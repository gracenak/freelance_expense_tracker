<h1>Welcome, <%= @user.username %>! </h1>
<br>
<h2>Your Gigs:</h2>

<% @user.gigs.each do |gig| %>
<ul>
<li><a href="/gigs/<%= gig.id %>"><%= gig.employer%></a></li>
<li><%= gig.date %></li>
<br><br>
</ul>
<% end %>
<br><br>

<form action="/gigs/new">
    <input type="submit" value="Create Gig" />
</form>
<br>
<form action="/logout">
    <input type="submit" value="Log Out">
</form>